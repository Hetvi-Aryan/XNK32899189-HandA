{
    "name": "Meet Mate",
    "description": "Your mate in Google Meets",
    "manifest_version": 2,
    "version": "0.2.4.0",
    "permissions": [
        "tabs",
        "activeTab"
        ,"https://meet.google.com/*",
        "storage",
        "notifications"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "lookout.png",
        "48": "lookout.png",
        "128": "lookout.png"
    },
    "content_scripts":[
        {
          "matches":["https://meet.google.com/*"],
          "js":["notes/jquery.min.js","notes/content.js"]
        }
    ]
}